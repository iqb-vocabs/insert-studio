{
  "name": "insert-studio",
  "version": "0.1.0",
  "author": "IQB - Institut zur Qualit√§tsentwicklung im Bildungswesen",
  "license": "MIT",
  "description": "Automatic metadata insertion from csv to Studio-lite",
  "scripts": {
    "create_json": "ts-node utils/readcsv.ts record.csv",
    "copy_to_fixtures": "cp utils/record.json cypress/fixtures/",
    "insert_record": "npx cypress open"
  },
  "homepage": "https://github.com/iqb-vocabs/insert-studio#readme",
  "eslintConfig": {
    "extends": "@iqb/eslint-config",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "no-console": "off",
      "class-methods-use-this": [
        "error",
        {
          "exceptMethods": [
            "getElementComponent",
            "getNewOptionLabel",
            "transform"
          ]
        }
      ],
      "import/export": 0
    }
  },
  "dependencies": {
    "@iqb/eslint-config": "^2.1.1",
    "csv-parse": "^5.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.7",
    "cypress": "^13.6.3",
    "tsx": "^4.7.0"
  }
}
